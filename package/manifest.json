{
  "manifest_version": 2,
  "name": "bookmark-manager",
  "version": "0.2.1",
  "icons": {
    "48": "icon.svg"
  },
  "browser_action": {
    "default_icon": "icon.svg"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+B",
        "mac": "MacCtrl+Command+B"
      }
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["bookmarks", "storage", "tabs"]
}
