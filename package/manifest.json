{
  "manifest_version": 2,
  "name": "bookmark-manager",
  "version": "0.0.0",
  "icons": {
    "48": "icon.svg"
  },
  "browser_action": {
    "default_icon": "icon.svg"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "ui.html",
    "open_in_tab": true
  },
  "permissions": ["bookmarks", "storage", "tabs"]
}
