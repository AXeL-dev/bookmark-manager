{
  "name": "bookmark-manager",
  "version": "0.5.1",
  "description": "Chrome Bookmark Manager for Firefox",
  "private": true,
  "scripts": {
    "start": "node scripts/clean-up && webpack-dev-server --config webpack.dev.js",
    "build": "node scripts/clean-up && webpack",
    "pack": "npm run build && web-ext build -s package -o && npm run zip-src",
    "ver": "node scripts/update-version",
    "zip-src": "node scripts/zip-source",
    "ext": "web-ext run -s package"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@reduxjs/toolkit": "^1.2.3",
    "lodash": "^4.17.15",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/firefox-webext-browser": "^70.0.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "babel-loader": "^8.0.6",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "web-ext": "^4.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
